<fest:template xmlns:fest="http://fest.mail.ru" context_name="data">
<fest:element name="div">
	<fest:attributes>	
        <fest:attribute name="class">scoreboard</fest:attribute>
    </fest:attributes>
	<table border="1" cellpadding="0" cellspacing="0">
		<thead>
			<tr>
				<th>Username</th>
				<th>E-mail</th>
				<th>Score</th>
			</tr>
		</thead>
		<tbody>
			<fest:for iterate="data" index="i" value="v">
				<tr>
					<td><fest:value>v.username</fest:value></td>
					<td><fest:value>v.email</fest:value></td>
					<td><fest:value>v.score</fest:value></td>
				</tr>
			</fest:for>
		</tbody>
	</table>
</fest:element>
</fest:template>
